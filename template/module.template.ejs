<%# Input param: submodule { program:'화면ID', code2:'소문자', code3:'소문자', url:'', name:'', desc:''}%>

(function(){
"use strict";

    angular.module('<%= program%>', [])
    .config(<%= program%>);

    <%= program%>.$inject = ['$stateProvider', '$urlRouterProvider', 'AppPath'];


    /**
    * <%= program%> 모듈 : <%= name%>
    * <%= desc%>
    * @memberof app
    * @ngdoc config
    * @name <%= program%>
    * @param $stateProvider {service} $stateProvider
    * @param $urlRouterProvider {service} $urlRouterProvider
    */
    function <%= program%> ($stateProvider, $urlRouterProvider, AppPath) {


        $stateProvider
            .state('tros.shell.<%= code2%>.<%= code3%>.<%= program%>', {
                url : '/<%= url%>',
                controller: '<%= program%>',
                controllerAs: 'vm',
                templateUrl: AppPath.<%= code2%> + '/<%= program%>.template.html'
            });


        $urlRouterProvider.when('/<%= topurl%>', '/<%= url%>');

    }

})();